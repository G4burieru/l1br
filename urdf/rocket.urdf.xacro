<?xml version="1.0" ?>
<robot name="rocket" xmlns:xacro="http://ros.org/wiki/xacro">

  <!--=========================== CONTROL ===========================-->
  <xacro:include filename="$(find rocket)/urdf/rocket_control.xacro" />

  <!--=========================== PLUGINS ===========================-->

  <!-- <xacro:include filename="$(find rocket)/urdf/rocket.gazebo.xacro" /> -->

  <!-- ****************** ROBOT CONSTANTS *******************************  -->
  <!-- Define the size of the robot's main chassis in meters -->
  <xacro:property name="base_width" value="0.47"/>
  <xacro:property name="base_length" value="0.77"/>
  <xacro:property name="base_height" value="0.13"/>
	
  <!-- Define the shape of the robot's two wheels in meters -->
  <xacro:property name="wheel_radius" value="0.07"/>
  <xacro:property name="wheel_width" value="0.03"/>

  <!-- Define the shape of the robot's tray in meters -->
  <xacro:property name="tray_radius" value="0.2"/>
  <xacro:property name="tray_width" value="0.041"/>
  <!-- ********************** ROBOT BASE *********************************  -->
  <link name="base_link">
    </link>

    <gazebo reference="chassis">
      <selfCollide>true</selfCollide>
      <mu1 value="0.3" />
      <mu2 value="0.3" />
      <kp value="1000000.0" />
      <kd value="100" />
      <fdir1 value="0 0 0" />
    </gazebo>
    
    <joint name="base_joint" type="fixed">
      <parent link="base_link"/>
      <child link="chassis" />
      <origin xyz="0.0 0.0 0.0118" rpy="0 0 3.14"/>
    </joint>

    <link name="chassis">
      <visual>
        <origin xyz="0  0  0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file:///$(find rocket)/meshes/EST_estrutura_m3.dae" />
        </geometry>
      </visual>

      <collision>
          <origin rpy="0  0  0" xyz="0  0  0.01" />
          <geometry>
              <box size="${base_length} ${base_width} ${base_height}"/>
          </geometry>
      </collision>

      <inertial>
          <origin xyz="0  0  0" rpy="0  1.57  0"/>
          <mass value="2.4"/>
          <inertia ixx="0.12196" ixy="0.0" ixz="0.0"
                iyy="0.16276" iyz="0.0"
                izz="0.14756" />
      </inertial>
    
    </link>

  <!-- *********************** DRIVE WHEELS ******************************  -->
    <gazebo reference="left_wheel">
      <selfCollide>false</selfCollide>
      <mu1>1.0</mu1>
      <mu2>1.0</mu2>
      <kp>1000000.0</kp>
      <kd>100.0</kd>
      <maxVel>1.0</maxVel>
      <fdir1 value="0 0 0" />
    </gazebo>

    <link name="left_wheel">
      <visual name="left_wheel_visual">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="file:///$(find rocket)/meshes/M_rodatrativa.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      
      <collision name="left_wheel_collision">
        <origin xyz="0 0 0" rpy="0 1.57 0"/>
        <geometry>
          <cylinder length="${wheel_width}" radius="${wheel_radius}"/>
        </geometry>
      </collision>
      
      <inertial>
        <mass value="1" />
        <origin rpy="0 1.57 0" xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0"
                iyy="0.001" iyz="0.0"
                izz="0.001" />
      </inertial>
      
    </link>

    <joint name="joint_left_wheel" type="continuous">
            <parent link="chassis" />
            <child link="left_wheel" />
            <origin rpy="0 0 1.57" xyz=" 0 -0.296755 0" />
            <axis rpy="0 0 0" xyz="1 0 0"/>
    </joint>

    <gazebo reference="right_wheel">
      <selfCollide>false</selfCollide>
      <mu1>1.0</mu1>
      <mu2>1.0</mu2>
      <kp>1000000.0</kp>
      <kd>100.0</kd>
      <maxVel>1.0</maxVel>
      <fdir1 value="0 0 0" />
    </gazebo>

    <link name="right_wheel">
      <visual name="right_wheel_visual">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="file:///$(find rocket)/meshes/M_rodatrativa.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      
      <collision name="right_wheel_collision">
        <origin xyz="0 0 0" rpy="0 1.57 0"/>
        <geometry>
          <cylinder length="${wheel_width}" radius="${wheel_radius}"/>
        </geometry>
      </collision>
      
      <inertial>
        <mass value="1" />
        <origin rpy="0 1.57 0" xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0"
                iyy="0.001" iyz="0.0"
                izz="0.001" />
      </inertial>
      
    </link>

    <joint name="joint_right_wheel" type="continuous">
            <parent link="chassis" />
            <child link="right_wheel" />
            <origin rpy="0 0 1.57" xyz="0 0.296395 0" />
            <axis rpy="0 0 0" xyz="1 0 0"/>
    </joint>

  <!-- *********************** TRAY ******************************  -->

  <link name="tray">
      <visual name="tray_visual">
        <origin xyz="0 0 0.0118" rpy="0 0 0"/>
        <geometry>
            <mesh filename="file:///$(find rocket)/meshes/ELV_conjuntocompleto.dae" scale="1 1 1"/>
        </geometry>
      </visual>
      
      <collision name="tray_collision">
        <origin xyz="0 -0.015047 0.1" rpy="0 0 0"/>
        <geometry>
          <cylinder length="${tray_width}" radius="${tray_radius}"/>
        </geometry>
      </collision>
      
      <inertial>
        <mass value="0.5" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.0" ixy="0.0" ixz="0.0"
                iyy="0.0" iyz="0.0"
                izz="0.0" />
      </inertial>
  </link>

  <joint name="joint_tray" type="fixed">
            <parent link="chassis" />
            <child link="tray" />
            <origin rpy="0 0 0" xyz="0 0.015047 0" />
  </joint>

  <!-- *********************** CASTER WHEELS ******************************  -->

  <gazebo reference="back_left_caster_wheel">
      <selfCollide>true</selfCollide>
      <mu1 value="0.09" />
      <mu2 value="0.09" />
      <kp value="10000000.0" />
      <kd value="10000" />
      <fdir1 value="0 0 0" />
  </gazebo>

  <link name="back_left_caster_wheel">
    <visual name="back_left_caster_wheel_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
      <material name="Black">
          <color rgba="${0/255} ${0/255} ${0/255} 1.0"/>
     </material>
    </visual>

    <collision name="back_left_caster_wheel_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
        <mass value="0.1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0" ixy="0.0" ixz="0.0"
                iyy="0" iyz="0.0"
                izz="0" />
      </inertial>
  </link>

  <joint name="joint_back_left_caster_wheel" type="fixed">
    <parent link="chassis"/>
    <child link="back_left_caster_wheel"/>
    <origin rpy="0 0 0" xyz="0.32 0.07 -0.06"/>
  </joint>

  <gazebo reference="back_right_caster_wheel">
      <selfCollide>true</selfCollide>
      <mu1 value="0.09" />
      <mu2 value="0.09" />
      <kp value="10000000.0" />
      <kd value="10000" />
      <fdir1 value="0 0 0" />
  </gazebo>

  <link name="back_right_caster_wheel">
    <visual name="back_right_caster_wheel_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
      <material name="Black">
          <color rgba="${0/255} ${0/255} ${0/255} 1.0"/>
     </material>
    </visual>

    <collision name="back_right_caster_wheel_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
        <mass value="0.1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0" ixy="0.0" ixz="0.0"
                iyy="0" iyz="0.0"
                izz="0" />
      </inertial>
  </link>

  <joint name="joint_back_right_caster_wheel" type="fixed">
    <parent link="chassis"/>
    <child link="back_right_caster_wheel"/>
    <origin rpy="0 0 0" xyz="0.32 -0.07 -0.06"/>
  </joint>

  <gazebo reference="front_left_caster_wheel">
      <selfCollide>true</selfCollide>
      <mu1 value="0.09" />
      <mu2 value="0.09" />
      <kp value="10000000.0" />
      <kd value="10000" />
      <fdir1 value="0 0 0" />
  </gazebo>

  <link name="front_left_caster_wheel">
    <visual name="front_left_caster_wheel_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
      <material name="Black">
          <color rgba="${0/255} ${0/255} ${0/255} 1.0"/>
     </material>
    </visual>

    <collision name="front_left_caster_wheel_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
        <mass value="0.1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0" ixy="0.0" ixz="0.0"
                iyy="0" iyz="0.0"
                izz="0" />
      </inertial>
  </link>

  <joint name="joint_front_left_caster_wheel" type="fixed">
    <parent link="chassis"/>
    <child link="front_left_caster_wheel"/>
    <origin rpy="0 0 0" xyz="-0.32 0.07 -0.06"/>
  </joint>

  <gazebo reference="front_right_caster_wheel">
      <selfCollide>true</selfCollide>
      <mu1 value="0.09" />
      <mu2 value="0.09" />
      <kp value="10000000.0" />
      <kd value="10000" />
      <fdir1 value="0 0 0" />
  </gazebo>

  <link name="front_right_caster_wheel">
    <visual name="front_right_caster_wheel_visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
      <material name="Black">
          <color rgba="${0/255} ${0/255} ${0/255} 1.0"/>
     </material>
    </visual>

    <collision name="front_right_caster_wheel_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </collision>

    <inertial>
        <mass value="0.1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0" ixy="0.0" ixz="0.0"
                iyy="0" iyz="0.0"
                izz="0" />
      </inertial>
  </link>

  <joint name="joint_front_right_caster_wheel" type="fixed">
    <parent link="chassis"/>
    <child link="front_right_caster_wheel"/>
    <origin rpy="0 0 0" xyz="-0.32 -0.07 -0.06"/>
  </joint>  

</robot>
